{
  "ApplicationName": "npgsqlrest",
  "EnvironmentName": "Production",
  "Urls": "http://0.0.0.0:8080",
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=postgres;Username=postgres"
  },
  "NpgsqlRest": {
    "UrlPathPrefix": "/api",
    "RequiresAuthorization": false,
    "KebabCaseUrls": true,
    "CamelCaseNames": true
  },
  "Log": {
    "MinimalLevels": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
